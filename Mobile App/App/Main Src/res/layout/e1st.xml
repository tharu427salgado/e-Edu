<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#37474F"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/ic_subject_white"
					android:scaleType="center" />
				<TextView
					android:id="@+id/textview4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:background="#455A64"
					android:gravity="center_horizontal|center_vertical"
					android:text="e-Edu LMS"
					android:textSize="20sp"
					android:textStyle="bold"
					android:textColor="#FFEB3B"
					android:singleLine="true"
					android:layout_gravity="center_horizontal|center_vertical" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear5"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="horizontal">
		<ScrollView
			android:id="@+id/vscroll1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:background="#FFFFFF">
			<LinearLayout
				android:id="@+id/linear6"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="vertical">
				<TextView
					android:id="@+id/textview5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="English Letters"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
				<TextView
					android:id="@+id/textview6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="සිංහල අකුරු"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
				<LinearLayout
					android:id="@+id/linear7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
				<LinearLayout
					android:id="@+id/linear8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
				<TextView
					android:id="@+id/textview7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="සිංහල ගීත"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
				<TextView
					android:id="@+id/textview8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="English Songs"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
					android:id="@+id/youtube6"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:autoPlay="false"
					app:useWebUi="false"
					app:showYouTubeButton="true"
					app:showFullScreenButton="true"
					app:showVideoCurrentTime="true"
					app:showVideoDuration="true"
					app:showSeekBar="true"
					app:handleNetworkEvents="false"
					app:enableAutomaticInitialization="true" />
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>
